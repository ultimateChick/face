        var setCookieYNote = function(k,v,d) {
            if (!d) {
                d = new Date(new Date().getTime()+3600*24*3600*1000).toGMTString();
            }
            document.cookie = k+"="+v+";expires="+d+";path=/;domain="+location.host.substring(4);
        };

        var getCookie = function(name) {
            var cookie = document.cookie;
            var cookies = cookie.split(";");
            for (var i=0;i<cookies.length;i++) {
                var kv = cookies[i].split("=");
                if (kv[0].replace(/(^\s*)|(\s*$)/g, "") == name) {
                    return kv[1] && kv[1].length?kv[1]:null;
                }
            }
            return false;
        };
        var info = getCookie("LAST_LOGIN");
        if (info) {
            userTxt.value = info;
            userTxt.style.color = "#000";
        };
        var authSuccess = function () {
            form.onsubmit = function () {};
            form.submit();
        };
        var listenCookie = (function () {
            var timer = null;
            return function (callback) {
                clearInterval(timer);
                timer = setInterval(function () {
                    var islogin = getCookie('YNOTE_LOGIN');
                    if (islogin) {
                        callback();
                        clearInterval(timer);
                    }
                }, 500);
            };
        })();

        var delegateAuth = function (e) {
            var evt = window.event ? window.event : e,
                target = evt.target || evt.srcElement,
                currentTarget = e ? e.currentTarget : this;
            evt.preventDefault && evt.preventDefault();
            if (target.tagName === 'A' || target.tagName === 'INPUT') {
                var className = target.className;
                _hmt.push(['_trackEvent', className + '-extention', 'click']);
                window.open(authUrl[className]);
                if (className !== 'download') {
                    listenCookie(authSuccess);
                }
            }
            return false;
        }
        openid.onclick = delegateAuth;

        var hideLoginTable = function () {
            _$('login-table').style.visibility = "hidden";
            _$('login-folder-up').style.visibility = "hidden";
            _$('login-more').style.visibility = "visible";
        }
        var showLoginTable = function () {
            _$('login-table').style.visibility = "visible";
            _$('login-folder-up').style.visibility = "visible";
            _$('login-more').style.visibility = "hidden";
        }
        folderTable.onclick = hideLoginTable;
        folderMore.onclick = showLoginTable;

        window.onload = function(){
            _hmt.push(['_trackEvent', 'login-page-extention', 'loadPage']);
        }
